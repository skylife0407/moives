<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ja.movie.moviePicture.mapper.PosterSQLMapper">


	<select id="posterPrint" resultType="com.ja.movie.vo.PosterVo">
		select * from poster 
		<![CDATA[where rownum < 2 and movie_no = #{movie_no} ]]> 
	</select>
	

</mapper>